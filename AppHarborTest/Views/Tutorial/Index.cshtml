@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link rel="Stylesheet" type="text/css" href="@Url.Content("~/Content/main.css")" />
    <link rel="Stylesheet" type="text/css" href="@Url.Content("~/Content/animate-custom.css")" />
    <link href='http://fonts.googleapis.com/css?family=Seaweed+Script' rel='stylesheet' type='text/css'>
    <script type="text/javascript" src="http://code.jquery.com/jquery-2.0.3.min.js"></script>
    <script type="text/javascript" src="@Url.Content("~/Content/app.js")"></script>
</head>
<body>
    <div id="container" class="center">
        <div id="logo" class="center">Alexa!</div>
        <div id="sections"></div>
        <div id="steps"></div>
        <div id="progress">
            <div class="progress progress-striped active">
                <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100" style="width: 0%">
                    <span class="sr-only"></span>
                </div>
            </div>
        </div>
        <div id="buttons">
            <button class="btn btn-info disabled" id="prevbtn" disabled>Προηγούμενο</button>
            <button class="btn btn-info disabled" id="nextbtn" disabled>Επόμενο</button>
        </div>
    </div>
    
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css">
    @*<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap-theme.min.css">*@
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
</body>
</html>
